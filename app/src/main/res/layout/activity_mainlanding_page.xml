<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainlandingPage">

    <ImageView
        android:id="@+id/seminarhall"
        android:layout_width="130dp"
        android:layout_height="121dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="72dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attendance"
        app:srcCompat="@drawable/rounded_border" />

    <ImageView
        android:id="@+id/logout"
        android:layout_width="130dp"
        android:clickable="true"
        android:layout_height="121dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/crud"
        app:srcCompat="@drawable/rounded_border" />

    <ImageView
        android:id="@+id/crud"
        android:layout_width="130dp"
        android:layout_height="121dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:srcCompat="@drawable/rounded_border" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/top_background" /><![CDATA[

    app:layout_constraintStart_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/imageView10" />



    ]]>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="136dp"
        android:layout_height="34dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="164dp"
        android:text="Welcome,"
        android:textColor="#E1A500"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/imageView10" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="341dp"
        android:layout_height="44dp"
        android:text="How may I help you today?"
        android:textSize="24sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="215dp" />

    <ImageView
        android:id="@+id/attendance"
        android:layout_width="130dp"
        android:layout_height="121dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:srcCompat="@drawable/rounded_border" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="94dp"
        android:layout_height="100dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="@+id/attendance"
        app:layout_constraintEnd_toEndOf="@+id/attendance"
        app:layout_constraintStart_toStartOf="@+id/attendance"
        app:layout_constraintTop_toTopOf="@+id/attendance"
        app:srcCompat="@drawable/images" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="96dp"
        android:layout_height="87dp"
        app:layout_constraintBottom_toBottomOf="@+id/crud"
        app:layout_constraintEnd_toEndOf="@+id/crud"
        app:layout_constraintStart_toStartOf="@+id/crud"
        app:layout_constraintTop_toTopOf="@+id/crud"
        app:srcCompat="@android:drawable/ic_menu_edit" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="111dp"
        android:layout_height="102dp"
        app:layout_constraintBottom_toBottomOf="@+id/seminarhall"
        app:layout_constraintEnd_toEndOf="@+id/seminarhall"
        app:layout_constraintStart_toStartOf="@+id/seminarhall"
        app:layout_constraintTop_toTopOf="@+id/seminarhall"
        app:srcCompat="@android:drawable/ic_menu_day" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="96dp"
        android:clickable="true"
        android:layout_height="99dp"
        app:layout_constraintBottom_toBottomOf="@+id/logout"
        app:layout_constraintEnd_toEndOf="@+id/logout"
        app:layout_constraintStart_toStartOf="@+id/logout"
        app:layout_constraintTop_toTopOf="@+id/logout"
        app:srcCompat="?attr/actionModeCloseDrawable" />
</androidx.constraintlayout.widget.ConstraintLayout>